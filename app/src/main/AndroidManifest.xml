<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.vehiclehmi">

    <queries>
        <package android:name="com.example.vehicleservice" />
    </queries>

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_settings_icon"
        android:label="@string/app_name"
        android:roundIcon="@drawable/ic_settings_icon"
        android:supportsRtl="true"
        android:theme="@style/Theme.VehicleHmi">
        <activity
            android:name=".View.MainActivity"
            android:exported="true"
            android:screenOrientation="landscape">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".View.TrailerTire"
            android:exported="true"
            android:screenOrientation="landscape"/>
        <activity android:name=".View.Keypad"
            android:exported="true"
            android:screenOrientation="landscape"/>
    </application>

</manifest>